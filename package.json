    "next": "8.1.0",
    "next-offline": "3.3.8",
    "next-transpile-modules": "2.3.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@types/next": "8.0.3",
